{
  "name": "@selfdb/js-sdk",
  "version": "1.0.1",
  "type": "module",
  "description": "Official JavaScript/TypeScript SDK for SelfDB - a self-hosted alternative to Supabase",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "selfdb",
    "database",
    "storage",
    "realtime",
    "authentication",
    "functions",
    "typescript",
    "javascript",
    "sdk"
  ],
  "author": "SelfDB Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Selfdb-io/js-sdk.git",
    "directory": "js-sdk"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^8.56.0",
    "jsdom": "^26.1.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0"
  },
  "dependencies": {
    "axios": "^1.6.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
